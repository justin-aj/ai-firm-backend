# Environment Configuration
DEBUG=True
API_HOST=0.0.0.0
API_PORT=8000

# LM Studio Configuration
LM_STUDIO_BASE_URL=http://127.0.0.1:1234/v1
LM_STUDIO_MODEL=local-model

# Google Custom Search API Configuration
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Create a Custom Search Engine at: https://programmablesearchengine.google.com/
GOOGLE_API_KEY=YOUR_API_KEY_HERE
GOOGLE_CX=YOUR_SEARCH_ENGINE_ID_HERE

# Dask Distributed Scraping Configuration
# Set to True to enable distributed scraping across multiple workers
USE_DASK=False
# Dask scheduler address (leave empty for local cluster)
# Example: localhost:8786 or scheduler.example.com:8786
DASK_SCHEDULER=
# Number of workers for local Dask cluster (ignored if DASK_SCHEDULER is set)
DASK_WORKERS=4

# Milvus Vector Database Configuration
# Milvus server host (default: localhost)
MILVUS_HOST=localhost
# Milvus server port (default: 19530)
MILVUS_PORT=19530
# Milvus collection name
MILVUS_COLLECTION=ai_firm_vectors

# Add your environment variables here
